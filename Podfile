# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MorseCodeApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Alamofire'
  pod 'SnapKit'
  pod 'RealmSwift'
  pod "RxRealm"
  pod 'Firebase'

  # Pods for MorseCodeApp

  target 'MorseCodeAppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxTest'
  end

  target 'MorseCodeAppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxTest'
  end

end
